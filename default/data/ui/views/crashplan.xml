  <row>
    <panel>
      <title>User Adoption</title>
      <chart>
        <title>Total Users Added Over Time</title>
        <search>
          <query>index="code42" sourcetype="c42userevent" earliest=-3d active=true username!=admin |
dedup userUid sortby -modificationDate |
eval UserCreationDate = substr(creationDate,1, 10) |
chart count by UserCreationDate |
accum count</query>
          <earliest>0</earliest>
        </search>
        <option name="charting.axisLabelsX.majorLabelStyle.overflowMode">ellipsisNone</option>
        <option name="charting.axisLabelsX.majorLabelStyle.rotation">0</option>
        <option name="charting.axisTitleX.visibility">visible</option>
